{
  "type": "symfony-bundle",
  "license": "MIT",
  "name": "suminagashi/orchestra",
  "description": "Sleek admin for your symfony projects",
  "keywords": [
    "admin",
    "backoffice",
    "spa"
  ],
  "version": "0.1.0",
  "authors": [
    {
      "name": "Louis Harang",
      "email": "pro@narah.io",
      "homepage": "https://narah.now.sh",
      "role": "Developer"
    },
    {
      "name": "Hugo Knorr",
      "email": "hugo.knorr@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "composer/package-versions-deprecated": "1.10.99.1",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.7",
    "symfony/asset": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/finder": "5.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "5.2.*",
    "symfony/framework-bundle": "5.2.*",
    "symfony/property-access": "5.2.*",
    "symfony/property-info": "5.2.*",
    "symfony/twig-bundle": "5.2.*",
    "symfony/webpack-encore-bundle": "^1.7",
    "symfony/yaml": "5.2.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "ext-simplexml": "*"
  },
  "require-dev": {
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Suminagashi\\OrchestraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrchestraBundleTests\\": "tests/"
    }
  }
}
